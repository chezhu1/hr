                              engage_type: this.InsertData.zplx,
                              regist_time: time,
                              deadline: this.InsertData.jzrq,
                              register: this.InsertData.name6,
                              major_describe: this.InsertData.zwms,
                              engage_required: this.InsertData.zpyq,
                          },
                      }).then(res => {
                          if (res.data > 0) {
                              alert("添加成功");
                              location.href = "/ZP/Zw_Index_Update";
                          }
                      }).catch(err => {
                          console.log(err);
                      })
                  }
              },
